<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" minHeight="-Infinity" minWidth="-Infinity" styleClass="primary-color"
      stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/17.0.2-ea">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="50.0">
            <children>
                <Label prefHeight="7.0" prefWidth="133.0" styleClass="primary-color" text="Feedback">
                    <font>
                        <Font name="Rockwell" size="17.0"/>
                    </font>
                    <HBox.margin>
                        <Insets left="5.0" top="6.0"/>
                    </HBox.margin>
                </Label>
                <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS"/>
            </children>
        </HBox>
        <Separator prefWidth="200.0">
            <VBox.margin>
                <Insets bottom="20.0"/>
            </VBox.margin>
        </Separator>
        <Label styleClass="primary-color" text="What do you think about our Akara Podcast?">
            <font>
                <Font size="15.0"/>
            </font>
            <VBox.margin>
                <Insets left="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
        <CheckBox mnemonicParsing="false" styleClass="primary-color" text="Great">
            <VBox.margin>
                <Insets left="10.0" top="5.0"/>
            </VBox.margin>
            <font>
                <Font size="15.0"/>
            </font>
        </CheckBox>
        <CheckBox mnemonicParsing="false" styleClass="primary-color" text="Good">
            <VBox.margin>
                <Insets left="10.0" top="9.0"/>
            </VBox.margin>
            <font>
                <Font size="15.0"/>
            </font>
        </CheckBox>
        <CheckBox mnemonicParsing="false" styleClass="primary-color" text="Not Bad">
            <VBox.margin>
                <Insets left="10.0" top="9.0"/>
            </VBox.margin>
            <font>
                <Font size="15.0"/>
            </font>
        </CheckBox>
        <CheckBox mnemonicParsing="false" styleClass="primary-color" text="Bad">
            <VBox.margin>
                <Insets left="10.0" top="9.0"/>
            </VBox.margin>
            <font>
                <Font size="15.0"/>
            </font>
        </CheckBox>
        <Label styleClass="primary-color" text="Recommend us more...">
            <font>
                <Font size="15.0"/>
            </font>
            <VBox.margin>
                <Insets left="10.0" top="25.0"/>
            </VBox.margin>
        </Label>
        <TextArea maxHeight="170.0" maxWidth="500.0" prefHeight="200.0" prefWidth="200.0">
            <VBox.margin>
                <Insets left="10.0" top="3.0"/>
            </VBox.margin>
        </TextArea>
        <Button mnemonicParsing="false" text="Submit">
            <VBox.margin>
                <Insets left="455.0" top="9.0"/>
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets left="24.0" right="24.0"/>
    </padding>
</VBox>
